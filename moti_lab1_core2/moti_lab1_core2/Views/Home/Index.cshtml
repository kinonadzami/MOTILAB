@{
    ViewData["Title"] = "Home Page";
}
<!doctype html>
<html>
<head>
    <title>Hello ASP.NET</title>
    <meta charset="utf-8" />
</head>
<body>
    <br />
    <a class="btn btn-primary" href="/Home/AddJudge">Добавить оценщика</a>
    <a class="btn btn-primary" href="/Home/AddAlternative">Добавить альтернатив</a>
    <a class="btn btn-primary" href="/Home/AddCriterion">Добавить критерий</a>
    <a class="btn btn-primary" href="/Home/AddMark">Добавить оценку</a>
    <a class="btn btn-primary" href="/Home/AddVector">Добавить вектор</a>

    <form action="Home/EditJudge" method="post">
        <input type="hidden" value="1" name="LNum" />
        <input type="submit" value="testEditJudge" />
    </form>
    <form action="Home/EditAlternative" method="post">
        <input type="hidden" value="1" name="ANum" />
        <input type="submit" value="testEditAlt" />
    </form>
    <form action="Home/EditVector" method="post">
        <input type="hidden" value="2" name="VNum" />
        <input type="submit" value="testEditVector" />
    </form>
    <form action="Home/EditMark" method="post">
        <input type="hidden" value="1" name="MNum" />
        <input type="submit" value="testEditMark" />
    </form>
    <form action="Home/EditCriterion" method="post">
        <input type="hidden" value="1" name="CNum" />
        <input type="submit" value="testEditCrit" />
    </form>
</body>
</html>